er=\"Priimek\"\n      ></b-form-input>\n    </b-form-group>\n\n    <b-form-group>\n      <b-form-input\n        class=\"input-field\"\n        id=\"address\"\n        v-model=\"form.address\"\n        required\n        placeholder=\"Naslov\"\n      ></b-form-input>\n    </b-form-group>\n\n    <b-form-group>\n      <b-form-textarea\n        class=\"input-field\"\n        id=\"content\"\n        v-model=\"form.content\"\n        required\n        rows=6\n        placeholder=\"Zakaj ste primerni za izbrano delo?\"\n      ></b-form-textarea>\n    </b-form-group>\n\n    <b-button type=\"submit\" class=\"btn-themed\">ODDAJ</b-button>\n  </b-form>\n</template>\n\n<script>\n  export default {\n    name: 'JobApplication',\n    data() {\n      return {\n        form: {\n          firstName: '',\n          lastName: '',\n          address: '',\n          content: '',\n        },\n      }\n    },\n    methods: {\n      onSubmit() {\n        if (!this.form) {\n          return;\n        }\n        console.log(this.form);\n        if (!localStorage.jobApplications) {\n          localStorage.jobApplications = JSON.stringify([]);\n        }\n        const parsedJobApplications = JSON.parse(localStorage.jobApplications)\n        parsedJobApplications.push(this.form);\n        const strJobApplications = JSON.stringify(parsedJobApplications);\n        localStorage.setItem('jobApplications', strJobApplications);\n        this.$emit('changeState', false);\n      }\n    }\n  }\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n  h4 {\n    color: #55b8c5;\n    font-family: 'Roboto', sans-serif;\n    font-weight: bold;\n  }\n\n  .btn-themed {\n    background-image: linear-gradient(to right, #55b8c5, #59a0ce);\n    width: 100%;\n    border: none;\n    font-weight: bold;\n  }\n\n  .btn-themed:hover {\n    background-image: linear-gradient(to right, #4cb6c4, #4f9bce);\n  }\n\n  .input-field {\n    border: 1px solid #55b8c5;\n  }\n\n</style>\n"